<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>

<HBox prefHeight="30.0" prefWidth="100.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <StackPane fx:id="update_pane" prefHeight="25.0" prefWidth="48.0">
         <children>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../media/icons/2x/update.png" />
               </image>
            </ImageView>
         </children>
      </StackPane>
      <Rectangle arcHeight="5.0" height="30.0" stroke="BLACK" strokeType="INSIDE" style="-fx-stroke: #D7DDE5; -fx-fill: #D7DDE5;" width="1.0">
         <HBox.margin>
            <Insets bottom="-1.0" />
         </HBox.margin>
      </Rectangle>
      <StackPane fx:id="delete_pane" layoutX="10.0" layoutY="10.0" prefHeight="25.0" prefWidth="48.0">
         <children>
            <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../media/icons/2x/trash.png" />
               </image>
            </ImageView>
         </children>
      </StackPane>
   </children>
</HBox>
